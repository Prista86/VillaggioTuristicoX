@{
    ViewData["Title"] = "AdminPage";
}

@using Microsoft.AspNetCore.Identity
@using MVC_TDPC13.DB.Entities
@inject UserManager<User> UserManager
@inject SignInManager<User> SignInManager


<h1>@ViewData["Title"]</h1>

<br>
<br>
<img id="imgAdmin" src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fwww.act.edu%2Fimages%2Fact%2FBusiness-Administration%2FIMG_0823DIG.jpg&f=1&nofb=1" />
<br>
<br>

<h4>Lista prenotazioni</h4>
<p>Di seguito la lista di tutte le prenotazioni</p>


@model List<PrenotazioneModel>;

<table class="table table-striped table-bordered">
    <tr class="table-primary">
        <th> Utente</th>
        <th> Suite</th>
    </tr>
    @foreach (PrenotazioneModel p in Model)
    {
        <tr>
            <td>@p.User</td>
            <td>@p.Suite</td>

        </tr>
    }
</table>



