@{
    ViewData["Title"] = "PrenotazioniUser";
}

@using Microsoft.AspNetCore.Identity
@using MVC_TDPC13.DB.Entities
@inject UserManager<User> UserManager
@inject SignInManager<User> SignInManager




<h1>Pagina Prenotazioni </h1>

<br />
<br />
<img id="imgPrenotazioni" src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fwww.naludamagazine.com%2Fwp-content%2Fuploads%2F2018%2F12%2Fnaluda-Relaxing-Vacation.jpg&f=1&nofb=1" />
<br>
<br>
<h4>Le tue prenotazioni</h4>
<br>

@model List<PrenotazioneModel>;

<table class="table table-striped table-bordered">
    <tr class="table-primary">
        <th> Utente</th>
        <th> Suite</th>
    </tr>
    @foreach (PrenotazioneModel p in Model)
    {
        <tr>
            <td>@p.User</td>
            <td>@p.Suite</td>

        </tr>
    }
</table>
